{
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
